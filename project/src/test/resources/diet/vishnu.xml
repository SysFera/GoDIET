<?xml version="1.0" encoding="UTF-8"?>
<godiet:dietPlatform xmlns:godiet="http://www.sysfera.com"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.sysfera.com ../Diet.xsd">

	<services>
		<omniNames id="omniNamesVishnu" domain="Domain1">
			<config server="Node1" remoteBinary="omniNames" />
		</omniNames>
	</services>

	<hierarchy>
		<masterAgent id="MA1">
			<config server="Node1" remoteBinary="dietAgent" />

			<sed id="sed1">
				<config server="VishnuGodiet" remoteBinary="ummssed" />
				<file id="umsseddiet" template="sed"></file>
				<file id="umssedconf" template="ummsed"></file>
				<commandLine>
					<parameter string="${file.ummsedconf.path}" />
				</commandLine>
			</sed>
			<sed id="sed2">
				<config server="VishnuGodiet" remoteBinary="immssed" />
				<file id="imsseddiet" template="sed"></file>
				<file id="imssedconf" template="immsed"></file>
				<commandLine>
					<parameter string="${file.imssedconf.path}" />
				</commandLine>
			</sed>
			<sed id="sed3">
				<config server="VishnuGodiet" remoteBinary="fmssed" />
				<file id="fmsseddiet" template="sed"></file>
				<file id="fmssedconf" template="fmssed"></file>
				<commandLine>
					<parameter string="${file.fmssedconf.path}" />
				</commandLine>
			</sed>
		</masterAgent>
	</hierarchy>
</godiet:dietPlatform>