<?xml version="1.0" encoding="UTF-8"?>
<dietDescription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="GoDietNG.xsd">

	<goDietConfiguration debug="1" saveStdOut="true" saveStdErr="true"
		log="true">
		<localscratch dir="/home/phi/tmp/scratch_godiet" />
	</goDietConfiguration>

		<infrastructure>
			<domain label="Sysfera">
				<storage label="kevinDisk">
					<scratch dir="/tmp/scratch_runtime" />
					<scp server="192.168.1.17" login="phi" />
				</storage>
				<storage label="phiDisk">
					<scratch dir="/home/phi/tmp/scratch_runtime" />
					<scp server="192.168.1.20" login="phi" />
				</storage>

				<node id="Kev" disk="kevinDisk">
					<ssh server="192.168.1.17" login="phi" />
					<env>
						<var name="PATH"
							value="/home/phi/Diet/build/bin:/home/phi/Diet/build/bin/examples/dmat_manips/:$PATH" />
						<var name="LD_LIBRARY_PATH" value="/home/phi/Diet/build/lib" />
					</env>
				</node>
				<node id="Phi" disk="phiDisk">
					<ssh server="192.168.1.20" login="phi" />
					<env>
						<var name="PATH"
							value="/home/phi/Soft/Diet/bin:/home/phi/Soft/Diet/bin/examples/dmat_manips/:$PATH" />
						<var name="LD_LIBRARY_PATH" value="/home/phi/Soft/diet/build/lib" />
					</env>
				</node>
				<gateway id="SysferaGW" ref="Kev" />
			</domain>
			<domain label="ENS">
				<storage label="graalDisk">
					<scratch dir="/tmp/scratch_runtime" />
					<scp server="140.77.166.19" login="pmartinez" />
				</storage>


				<node id="Graal" disk="graalDisk">
					<ssh server="140.77.166.19" login="pmartinez" />
					<env>
						<var name="PATH"
							value="/home/pmartinez/Diet/build/bin:/home/pmartinez/Diet/build/bin/examples/dmat_manips/:$PATH" />
						<var name="LD_LIBRARY_PATH"
							value="/home/pmartinez/Diet/build/lib:/home/bdepardo/soft/omniORB/lib" />
					</env>
				</node>
				<gateway id="ENSGW" ref="Graal" />

			</domain>

			<link from="SysferaGW" to="ENSGW" />
		</infrastructure>

	<dietServices>
		<omniNames id="omniNamesDomain1">
			<config server="Kev" remoteBinary="omniNames" />
		</omniNames>
		<omniNames id="omniNamesDomain2">
			<config server="Graal" remoteBinary="omniNames" />
		</omniNames>
	</dietServices>

	<dietInfrastructure>
		<masterAgent id="ma1">
			<config server="Kev" remoteBinary="dietAgent"/>
			<localAgent id="la1">
				<config server="Graal" remoteBinary="dietAgent" />
				<sed id="sed1">
					<config server="Graal" remoteBinary="dmat_manips_server" />
					<parameters string="T" />
				</sed>
			</localAgent>
		</masterAgent>
	</dietInfrastructure>

</dietDescription>
