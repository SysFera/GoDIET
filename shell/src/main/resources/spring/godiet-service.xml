<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
         http://www.springframework.org/schema/context
         http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<bean id="godiet" class="com.sysfera.godiet.common.services.GodietServiceProxyImpl">
		<property name="configurationService" ref="configurationServiceRMIProxy"/>
		<property name="infrastructureService" ref="infrastructureServiceRMIProxy"/>
		<property name="platformService" ref="platformServiceRMIProxy"/>
		<property name="userService" ref="userServiceRMIProxy"/>
		<property name="xMLLoaderService" ref="xMLLoaderRMIProxy"/>
	</bean>

	
	
	
</beans>
